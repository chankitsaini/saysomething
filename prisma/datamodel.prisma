type User {
  id: ID! @unique
  googleId: String @unique
  displayName: String
  email: String! @unique
  password: String
  profileImageUrl: String
  posts: [Post!]!
}

type Post {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  published: Boolean! @default(value: "false")
  title: String!
  content: String
  author: User!
}
